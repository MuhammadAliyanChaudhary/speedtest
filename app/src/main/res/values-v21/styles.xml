<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="sflash" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:statusBarColor" tools:ignore="NewApi">@android:color/black</item>
        <item name="android:navigationBarColor" tools:ignore="NewApi">@android:color/black</item>
    </style>

    <style name="sflashgreenex" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/newbg</item>
        <item name="colorPrimaryDark">@color/headerTextColour</item>
        <item name="android:windowActivityTransitions">true</item>
        <item name="colorAccent">@color/headerTextColour</item>
        <item name="android:statusBarColor" tools:ignore="NewApi">@color/newbg</item>
        <item name="android:navigationBarColor" tools:ignore="NewApi">@color/newbg</item>
    </style>

</resources>